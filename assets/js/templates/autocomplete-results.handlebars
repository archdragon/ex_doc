<div class="autocomplete-results">
  <a class="autocomplete-result" href="search.html?q={{term}}" data-index="-1">
    <div class="title">"<em>{{term}}</em>"</div>
    <div class="description">Search the documentation</div>
  </a>
  {{#each results}}
    <a href="{{moduleTitle}}.html#{{anchor}}" class="autocomplete-result" data-index="{{@index}}">
      <div class="title">
        {{{title}}}
        {{#if label}}
          <span class="label">({{label}})</span>
        {{/if}}
      </div>

      {{#if description}}
        <div class="description">
          {{{description}}}
        </div>
      {{/if}}
    </a>
  {{/each}}
</div>
